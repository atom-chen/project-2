                  </AbstractNodeData>
                  <AbstractNodeData Name="Text_4_0" ActionTag="-419886961" Tag="87" IconVisible="False" LeftMargin="19.3877" RightMargin="440.6123" TopMargin="163.5089" BottomMargin="1128.4911" FontSize="28" LabelText="[牌型判断]" ShadowOffsetX="2.0000" ShadowOffsetY="-2.0000" ctype="TextObjectData">
                    <Size X="140.0000" Y="28.0000" />
                    <AnchorPoint ScaleX="0.5083" ScaleY="0.3683" />
                    <Position X="90.5497" Y="1138.8035" />
                    <Scale ScaleX="1.0000" ScaleY="1.0000" />
                    <CColor A="255" R="253" G="230" B="134" />
                    <PrePosition X="0.1509" Y="0.8627" />
                    <PreSize X="0.2333" Y="0.0212" />
                    <OutlineColor A="255" R="255" G="0" B="0" />
